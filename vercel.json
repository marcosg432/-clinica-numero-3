{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/config.js",
      "destination": "/public/config.js"
    },
    {
      "source": "/sobre",
      "destination": "/sobre.html"
    },
    {
      "source": "/tratamentos",
      "destination": "/tratamentos.html"
    },
    {
      "source": "/agendamento",
      "destination": "/agendamento.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/admin-login.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

